<view class="page">
    <view class="main_lb">
        <view class= "main_list_cdxx main_list_first">
            <view class="main_param">
              电能表编号：
            </view>
            <view class="main_value">
             {{dnbbh}}
            </view>
        </view>
          <view class= "main_list_cdxx">
            <view class="main_param">
              拆表示值：
            </view>
            <view class="main_input">
              <input placeholder="请输入拆表示值" bindinput="inputTyping"/>
            </view>
            <view class="main_dw">
              （kWh）
            </view>
        </view>
          <view class= "main_list_cdxx main_list_second">
              <view class="main_param">
                设备编号：
              </view>
              <view class="main_value">
                {{stInfo.chargeDevNo}}
              </view>
          </view>

          <view class= "main_list_cdxx">
              <view class="main_param">
                充电站编号：
              </view>
              <view class="main_value">
               {{stInfo.stationNo}}
              </view>
          </view>

          <view class= "main_list_cdxx">
              <view class="main_param">
                充电站名称：
              </view>
              <view class="main_value">
                {{stInfo.name}}
              </view>
          </view>

          <view class= "main_list_cdxx">
              <view class="main_param">
                充电站地址：
              </view>
              <view class="main_value">
                {{stInfo.stationAddr}}
              </view>
          </view>
      </view>
      <view class= "main_list_btn" bindtap='unbindMeter'>
          <view class="main_list_btn_text">
              拆除
          </view>
      </view>
</view>
